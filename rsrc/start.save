//make_object creates an instance of an "object".obj.
make_object "player" { //forces a load of player.obj
  id="player"
  //the starting map
  map="cage" //forces a load of cage.map
  //the position vector
  position=[1,1,0]
  //the facing vector. this means "to the right"
  facing=[1,0,0]
}
//other "start of game" overrides