save {
	map "cage" {
  	//make_object creates an instance of an "object".obj.
  	make_object "player" { //forces a load of player.obj
    	id="player"
    	//the position vector
    	position=[5,5,0]
    	//the facing vector. this means "down"
    	facing=[0,1,0]
  	}
  	make_object "stone" { //forces a load of stone.obj
    	id="cage_stone"
    	position=[3,6,0]
    	facing=[1,0,0]
  	}
	}
  //other "start of game" overrides
}
